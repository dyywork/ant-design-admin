{
  "name": "ant-design-admin",
  "version": "0.0.0",
  "author": "dingyongya@qq.com",
  "scripts": {
    "commit": "git add . && git-cz && git push && git push github master",
    "dev": "vite",
    "build": "vue-tsc --noEmit && vite build",
    "preview": "vite preview",
    "prettier": "prettier --write \"./**/*.{vue,ts,js}\"",
    "lint": "eslint --ext .vue,.ts src",
    "prepare": "husky install"
  },
  "dependencies": {
    "@ant-design/icons-vue": "^6.1.0",
    "ant-design-vue": "^3.2.9",
    "axios": "^0.26.0",
    "eslint": "^6.0.0",
    "vue": "^3.2.25",
    "vue-router": "^4.0.12",
    "vuex": "^4.0.2"
  },
  "devDependencies": {
    "@commitlint/cli": "^16.2.1",
    "@commitlint/config-conventional": "^16.2.1",
    "@commitlint/prompt-cli": "^16.2.1",
    "@types/mockjs": "^1.0.6",
    "@typescript-eslint/parser": "^5.10.0",
    "@vitejs/plugin-vue": "^2.0.0",
    "@vue/cli-plugin-eslint": "^4.5.15",
    "cz-conventional-changelog": "^3.3.0",
    "eslint-config-prettier": "^8.0.0",
    "eslint-plugin-vue": "^8.4.1",
    "husky": "^7.0.4",
    "less": "^4.1.2",
    "mockjs": "^1.1.0",
    "prettier": "2.5.1",
    "typescript": "^4.4.4",
    "vite": "^2.7.2",
    "vue-eslint-parser": "^8.2.0",
    "vue-tsc": "^0.29.8"
  },
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog",
      "types": {
        "feat": {
          "description": "A new feature(一个新功能)",
          "title": "Features"
        },
        "fix": {
          "description": "A bug fix(错误修复)",
          "title": "Fix"
        },
        "docs": {
          "description": "Documentation only changes(仅文档更改)",
          "title": "Docs"
        },
        "style": {
          "description": "Changes that do not affect the meaning of the code(不影响代码含义的更改)",
          "title": "Style"
        },
        "refactor": {
          "description": "A code change that neither fixes a bug nor adds a feature(既不修复错误也不添加功能的代码更改)",
          "title": "Refactor"
        },
        "perf": {
          "description": "A code change that improves performance(提高性能的代码更改)",
          "title": "Perf"
        },
        "test": {
          "description": "Adding missing tests or correcting existing tests(添加缺失的测试或纠正现有的测试)",
          "title": "Test"
        },
        "build": {
          "description": "Changes that affect the build system or external dependencies(影响构建系统或外部依赖项的更改)",
          "title": "Build"
        },
        "ci": {
          "description": "Changes to our CI configuration files and scripts(更改我们的 CI 配置文件和脚本)",
          "title": "Ci"
        },
        "chore": {
          "description": "Other changes that don't modify src or test files(不修改 src 或测试文件的其他更改)",
          "title": "Chore"
        },
        "revert": {
          "description": "Reverts a previous commit(恢复之前的提交)",
          "title": "Revert"
        }
      }
    }
  }
}
